function publishHeight()
{
    if (window.location.hash.length == 0) return;

    var flakeId = getFlakeId();
    var bodyHeight = PU.getPosition($('bottom'))[1]; //PU.getContentHeight();
    var viewHeight = PU.getViewportHeight();

    if( Math.abs( bodyHeight - viewHeight ) > 15 )
    {
        var parentUrl = window.location.hash.substring(1);

        parentUrl += "#";
        parentUrl += 'FlakeId=' + flakeId;
        parentUrl += '&';
        parentUrl += 'Height=' + bodyHeight.toString();

        //Top Does not work, Throws Access denied.
        try
        {
            top.location = parentUrl;
            document.body.scroll = "no";
        }
        catch( e )
        {
            document.body.scroll = "yes";
        }
        //parent.location = parentUrl;
    }
}

function getFlakeId()
{
    var qs = parseQueryString(window.location.href);
    var flakeId = qs["FlakeId"];
    var hashIndex = flakeId.indexOf('#');

    if (hashIndex > -1)
    {
        flakeId = flakeId.substring(0, hashIndex);
    }

    return flakeId;
}

function parseQueryString(url)
{
    url = new String(url);
    var queryStringValues = new Object();
    var querystring = url.substring((url.indexOf('?') + 1), url.length);
    var querystringSplit = querystring.split('&');

    for (i = 0; i < querystringSplit.length; i++)
    {
        var pair = querystringSplit[i].split('=');
        var name = pair[0];
        var value = pair[1];

        queryStringValues[name] = value;
    }

    return queryStringValues;
}
